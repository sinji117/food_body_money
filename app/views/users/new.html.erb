<%= form_with(model: @user, local: true) do |form| %>
  <div class="title">
    <%= form.label :nickname %>
    <%= form.text_field :nickname %>
  </div>
  <div class="time">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time %>
  </div>
  <div class="submit">
    <%= form.submit %>
  </div>
<% end %>
